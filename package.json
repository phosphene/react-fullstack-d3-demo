  "babelConfig": {
    "server": {
      "presets": [
        "es2015",
        "stage-0",
        "react"
      ],
      "plugins": [
        "transform-decorators-legacy"
      ]
    },
    "client": {
      "presets": [
        "es2015",
        "stage-0",
        "react"
      ],
      "plugins": [
        "transform-decorators-legacy"
      ],
      "env": {
        "development": {
          "plugins": [
            "react-hot-loader/babel"
          ]
        }
      }
    }
  },
  "config": {
    "assetsPublicPath": "/assets/"
  }
}
